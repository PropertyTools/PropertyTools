<Page x:Class="ExampleBrowser.Pages.UncategorizedExample"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:pt="clr-namespace:PropertyTools.Wpf;assembly=PropertyTools.Wpf.Obsolete" mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
	Title="UncategorizedExample">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <pt:PropertyEditor Margin="8" SelectedObject="{Binding TestObject}"/>
        <pt:PropertyEditor Grid.Column="2" Margin="8" SelectedObject="{Binding TestObject}" UseDefaultCategoryNameForUncategorizedProperties="True" DefaultCategoryName="Misc"/>
    </Grid>
</Page>
